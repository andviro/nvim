{
  "runtime.version": "LuaJIT",
  "runtime.path": [
    "lua/?.lua",
    "lua/?/init.lua"
  ],
  "diagnostics.globals": [
    "vim"
  ],
  "workspace.checkThirdParty": false,
  "workspace.library": [
    "$VIMRUNTIME",
    "$XDG_CONFIG_HOME/nvim",
    "$HOME/.local/share/nvim/site",
    "$HOME/.local/share/nvim/lazy",
  ]
}
